term.clear()
term.setCursorPos(1,1)
print("input url:")
term.setCursorPos(1,2)
fileurl = read()
term.setCursorPos(1,3)
print("input program name:")
term.setCursorPos(1,4)
programName = read()
page = http.get(fileurl)
program = page.readAll()
file = ls.open(programName,"w")
file.write(program)
file.close()
